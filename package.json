{
  "name": "bootstrap-font-awesome-boilerplate",
  "version": "1.0.2",
  "description": "This is a simple starter pack for setting up your webpack for Bootstrap and FontAwesome",
  "main": "index.js",
  "scripts": {
    "start": "yarn dev",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --open --config webpack.config.js",
    "build": "webpack -p --progress --mode production --config webpack.config.js",
    "lint": "eslint src/js",
    "lint:fix": "yarn lint -- --fix",
    "format": "prettier --write \"./**/*.{js,html,css,scss,json}\""
  },
  "author": "Deniz GÃ¼zel",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "bootstrap": "^4.5.2",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "css-url-relative-plugin": "^1.0.0",
    "eslint": "^7.8.1",
    "file-loader": "^6.1.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.4.1",
    "image-webpack-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.11.1",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.0.1",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "prettier": "^2.1.1",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.3"
  }
}
